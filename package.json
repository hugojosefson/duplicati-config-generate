{
  "name": "duplicati-generate-config",
  "version": "1.0.0",
  "description": "Generate duplicati backup configs from a flat file",
  "main": "src/api-commonjs.js",
  "module": "src/api.js",
  "files": [
    "bin",
    "src"
  ],
  "bin": {
    "duplicati-generate-config": "bin/duplicati-generate-config.js"
  },
  "scripts": {
    "start": "bin/duplicati-generate-config.js",
    "test": "run-s lint mocha",
    "mocha": "nyc mocha",
    "lint": "standard",
    "lint-fix": "standard --fix",
    "prepare": "run-s documentation",
    "documentation": "documentation readme --section=API --shallow --format=md src/api.js",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "precommit": "lint-staged"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "cache": true,
    "all": true
  },
  "lint-staged": {
    "*.{mjs,js,jsx}": [
      "standard --fix",
      "git add"
    ]
  },
  "keywords": [
    "duplicati",
    "config",
    "backup"
  ],
  "author": "Hugo Josefson <hugo@josefson.org> (https://www.hugojosefson.com/)",
  "license": "ISC",
  "comment-engines.node": "Not using babel, code seems to work from v8.3.0 and up.",
  "engines": {
    "node": ">=8.3.0"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      [
        "transform-object-rest-spread"
      ]
    ]
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "documentation": "^5.3.2",
    "husky": "^0.14.3",
    "lint-staged": "^6.0.0",
    "mocha": "^4.0.1",
    "npm-run-all": "^4.1.1",
    "nyc": "^11.2.1",
    "ramda": "^0.25.0",
    "standard": "^10.0.3"
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.26.0",
    "engine-check": "^1.0.0",
    "term-size": "^1.2.0",
    "yargs": "^10.0.3"
  }
}
