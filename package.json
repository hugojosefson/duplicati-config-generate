{
  "name": "duplicati-config-generate",
  "version": "1.0.0",
  "description": "Generate duplicati backup configs from a flat file",
  "main": "src/index.mjs",
  "files": [
    "src",
    "yarn.lock"
  ],
  "scripts": {
    "start": "node --experimental-modules index.mjs",
    "test": "run-p lint",
    "lint": "standard",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.mjs": [
      "standard --fix",
      "git add"
    ]
  },
  "keywords": [
    "duplicati",
    "config",
    "backup"
  ],
  "author": "Hugo Josefson <hugo@josefson.org>",
  "license": "ISC",
  "engines": {
    "node": ">=8.6"
  },
  "devDependencies": {
    "husky": "^0.14.3",
    "lint-staged": "^4.2.3",
    "npm-run-all": "^4.1.1",
    "standard": "^10.0.3"
  },
  "dependencies": {
    "highland": "^2.11.1"
  }
}
